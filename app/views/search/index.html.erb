<h1><%= "#{@stores.count}" %> Total Stores</h1>

<% @stores.each do |store| %>
  <div class="store">
    <p class="name"><%= "#{store.name}" %></p>
    <p class="city"><%= "#{store.city}" %></p>
    <p class="distance"><%= "#{store.distance}" %></p>
    <p class="phone_number"><%= "#{store.phone_number}" %></p>
    <p class="store_type"><%= "#{store.store_type}" %></p>
  </div>
<% end %>